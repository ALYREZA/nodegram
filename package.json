    "api",
    "node",
    "instagram-node",
    "instagram-node-lib"
  ],
  "homepage": "https://github.com/zzarcon/nodegram",
  "dependencies": {
    "rsvp": "^3.1.0",
    "superagent": "^1.7.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^2.4.5"
  }
